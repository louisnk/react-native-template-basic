{
  "name": "rn_template_basic",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "clean": "watchman watch-del-all && rm -rf node_modules && yarn && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-*",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "flow": "flow",
    "flow-stop": "flow stop",
    "ios": "react-native run-ios",
    "lint": "eslint --ext .js --ext .jsx  --ignore-path .gitignore --ignore-pattern 'node_modules/**/*' ./",
    "lint-fix": "eslint --fix ./src",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.6.1",
    "react-native": "0.57.5",
    "react-native-gesture-handler": "^1.0.9",
    "react-navigation": "^3.0.0",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-persist": "^5.10.0",
    "redux-persist-seamless-immutable": "^2.0.0",
    "redux-saga": "^0.16.2",
    "reduxsauce": "^1.0.0",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "flow-bin": "^0.78.0",
    "husky": "^1.1.4",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.49.2",
    "react-test-renderer": "16.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-fix",
      "pre-push": "yarn test",
      "post-pull": "yarn"
    }
  },
  "jest": {
    "preset": "react-native"
  }
}
